{
  "name": "@appbaseio/reactivebase",
  "version": "1.0.0-alpha19",
  "description": "Data components for building reactive UIs",
  "main": "lib/app.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "esw app --color --watch",
    "webpack-server": "webpack-dev-server --hot --inline --progress --colors --port 8012",
    "web-server": "http-server -p 3000 .",
    "start": "cross-env CHOOSE_CONFIG=EXAMPLES && concurrently \"npm run webpack-server\" \"gulp watch\" ",
    "build": "cross-env NODE_ENV=production && cross-env CHOOSE_CONFIG=EXAMPLES && webpack -p && gulp",
    "build:lib": "gulp && cross-env NODE_ENV=production && cross-env CHOOSE_CONFIG=LIB babel app --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development && webpack --config webpack.config.umd.js app/app.js umd/reactivebase.js",
    "build-bro": "browserify -t [ babelify --presets [ es2015 stage-0 react ] ] main.js -o dist/main.bundle.js",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook -o playground",
    "build:all": "npm run build && npm run build:lib && npm run build:umd && npm run build-storybook"
  },
  "jest": {
    "rootDir": "./tests"
  },
  "files": [
    "lib/",
    "dist/",
    "umd/",
    "examples/"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "appbase-js": "^0.11.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "fbemitter": "^2.0.2",
    "jquery": "^3.1.1",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "rc-slider": "^5.4.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dates": "^4.3.0",
    "react-dom": "^15.4.2",
    "react-moment-proptypes": "^1.3.0",
    "react-select": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "@appbaseio/reactivemaps-manual": "1.0.0-alpha",
    "@kadira/storybook": "^2.34.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addon-options": "^1.0.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.7",
    "browserify": "^13.1.1",
    "browserify-shim": "^3.8.12",
    "concurrently": "^2.0.0",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.10.6",
    "materialize-css": "^0.98.0",
    "raw-loader": "^0.5.1",
    "react-scripts": "^0.7.0",
    "react-test-renderer": "^15.4.2",
    "shebang-loader": "0.0.1",
    "storybook-readme": "^1.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "1.16.0"
  }
}
